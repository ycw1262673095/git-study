page{
  background-color: #f6f6f6;
  padding: 0px;
}
.header {
  height: 350rpx;
}
.logo {
  display: block;
  width: 100%;
  height: 350rpx;
  margin: 0rpx auto;
  margin-bottom:10rpx;
}
.name {
  font-size: 13pt;
  text-align: center;
  line-height: 150%;
}
.describe {
  width:100%;
  box-sizing:border-box;
  padding:20rpx 45rpx 30rpx;
  display:flex;
  flex-flow:column nowrap;
  color:#545454;
  line-height:175%;
  background:#fff;
  margin-top:15rpx;
  border-top:1px #e5e5e5 solid;
  border-bottom:1px #e5e5e5;
}
.des-list {
  position: relative;
  display:flex;
  flex-direction:column;
  border-left:8rpx solid #ddd;
  padding-left:15rpx;
  color:#777;
  margin-bottom:15rpx;
  font-size:10pt;
}
.list-left {
  color: gray;
}

.des-content {
  padding-bottom:15rpx;
}
