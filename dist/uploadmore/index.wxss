.uploadPic_list{
  display: flex;
  flex-wrap: wrap;
}
.uploadPic_box{
  position: relative;
  width: 150rpx;
  height: 150rpx;
  margin-right: 18rpx;
  margin-top: 18rpx;
}
.uploadPic_box:nth-child(-n+3){
  margin-top: 0px !important;
}
.uploadPic_box .uploadPic {
  border: 1px  dashed #efefef;
  border-radius: 4rpx;
  width: 100%;
  height: 100%;
}
.removestuphoto{
  position: absolute;
  top: -12rpx;
  right: -12rpx;
  width: 30rpx;
  height: 30rpx;
}

